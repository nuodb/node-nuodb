{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": 0,
    "indent": [
      "error",
      2
    ],
    "no-multi-spaces": 2,
    "no-tabs": 2,
    "no-trailing-spaces": 2,
    "no-mixed-spaces-and-tabs": "error"
  }
}
